{
    "manifest_version": 3,
    "name": "Automated Answer Extension",
    "version": "1.0",
    "description": "Automatically answers questions on a web platform.",
    "permissions": ["scripting", "activeTab"],
    
    
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["content.js", "popup.js"]
        }
    ]
}
